"
run/main_scene="res://Systems/MainMenu/Menu.tscn"
config/use_custom_user_dir=true
config/icon="res://tablordia.svg"
config/quit_on_go_back=false

[autoload]

Std="*res://Systems/std/std.tscn"
NetworkInterface="*res://Systems/networking/network_interface.tscn"
List="*res://Systems/networking/list.tscn"
CMD="*res://Systems/cmd/cmd.tscn"
Spawner="*res://Systems/Spawner/Spawner.tscn"
UMB="*res://Systems/UMB/UMB.tscn"
HUD="*res://Systems/HUD/HUD.tscn"
RCM="*res://Systems/RCM/right_click_menu.tscn"
SettingsUI="*res://Systems/Settings/SettingsUI.tscn"

[debug]

multirun/number_of_windows=1
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1280
window/size/test_height=720
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/multirun/plugin.cfg" )

